{
  "name": "Rockstagram",
  "version": "0.1.0",
  "description": "Automatically follow people in your niche, write comments, clean useless followers, write private messages and much more",
  "author": "Rockstagram",
  "icon": "public/icon.png",
  "private": true,
  "dependencies": {
    "electron-updater": "^4.0.6",
    "insta-workers": "git@github.com:Rockstagram/workers.git"
  },
  "main": "public/electron.js",
  "homepage": ".",
  "repository": "https://github.com/Rockstagram/app",
  "scripts": {
    "electron": "MODE=development electron .",
    "prestart": "npm install",
    "postinstall": "electron-builder install-app-deps",
    "electron-dev": "npm start | nodemon --watch electron --exec 'ELECTRON_START_URL= MODE=development electron' .",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "pack": "build --mac",
    "preship": "npm run build",
    "ship": "build --mac -p always"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "electron": "^4.0.2",
    "electron-devtools-installer": "^2.2.4",
    "nodemon": "^1.18.9",
    "electron-builder": "^20.38.5",
    "redux-devtools-extension": "^2.13.7"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
