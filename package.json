{
  "name": "rockstagram",
  "version": "0.2.0",
  "description": "Automatically follow people in your niche, write comments, clean useless followers, write private messages and much more",
  "author": "Rockstagram",
  "private": true,
  "main": "public/electron.js",
  "homepage": ".",
  "repository": "https://github.com/Rockstagram/app",
  "scripts": {
    "prestart": "npm install",
    "postinstall": "electron-builder install-app-deps",
    "electron-solo": "MODE=development electron .",
    "electron": "nodemon --watch electron --exec 'ELECTRON_START_URL=http://localhost:3000/ MODE=development electron' .",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "pack": "npm run build && npm run pack-all",
    "pack-mac": "build --mac --config buildScripts/electron-builder.yml",
    "pack-win-32": "build --win --ia32 --config buildScripts/electron-builder.yml",
    "pack-linux": "build --linux --config buildScripts/electron-builder.yml",
    "pack-all": "build -mwl --config buildScripts/electron-builder.yml",
    "ship": "npm run build & npm run ship-all",
    "ship-mac": "build --mac --config buildScripts/electron-builder.yml -p always",
    "ship-all": "build -mwl --config buildScripts/electron-builder.yml -p always"
  },
  "dependencies": {
    "electron-dl": "^1.13.0",
    "electron-updater": "^4.0.6",
    "extract-zip": "^1.6.7",
    "firebase": "^5.8.2",
    "insta-workers": "git@github.com:Rockstagram/workers.git",
    "node-fetch": "^2.3.0",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.14",
    "@fortawesome/free-solid-svg-icons": "^5.7.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "electron": "^4.0.3",
    "electron-devtools-installer": "^2.2.4",
    "nodemon": "^1.18.9",
    "electron-builder": "^20.38.5",
    "redux-devtools-extension": "^2.13.7"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
